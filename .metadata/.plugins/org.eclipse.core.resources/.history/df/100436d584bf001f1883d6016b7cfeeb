package VariableStatic;

public class Varclass {

	String name;
	String address;
	String city;
	
	public Varclass(String name, String address, String city) {
		
	this.name=name;
	this.address=address;
	this.city=city;
	
	}
	
	public void getAdress() {
	
	System.out.println(address);
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Varclass obj=new Varclass("Bob","Indiranagar","Bangalore");
		Varclass obj1=new Varclass("Ram","Halasuru","Bangalore");
		obj.getAdress();
		obj1.getAdress();
		
		
	}

}
